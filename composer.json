{
    "name": "kahlan/extra-matcher",
    "description": "Additional matchers for Kahlan.",
    "license": "MIT",
    "authors": [
        {
            "name": "CrysaLEAD"
        }
    ],
    "require": {
        "php": ">=5.4"
    },
    "require-dev": {
        "kahlan/kahlan": "~3.0|4.0.x-dev",
        "squizlabs/php_codesniffer": "~2.7"
    },
    "autoload": {
        "psr-4": {
            "Kahlan\\Extra\\Matcher\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kahlan\\Extra\\Matcher\\Spec\\": "spec/"
        }
    },
    "config": {
        "bin-dir": "bin"
    }
}
